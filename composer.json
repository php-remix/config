{
    "name": "php-remix/config",
    "description": "Config manager for php-remix framework",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "mtdowling/jmespath.php": "^2.6"
    },
    "require-dev": {
        "php-remix/framework": "dev-master",
        "symfony/var-dumper": "^5.2",
        "phpunit/phpunit": "^8.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "moqiaoduo",
            "email": "415372363@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpRemix\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "extra": {
        "php-remix": {
            "di": "src\\di-config.php"
        }
    }
}
